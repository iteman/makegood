<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.piece_framework.makegood.ui.definitions.phpContentType">
         <with
               variable="activeContexts">
            <iterate
                  ifEmpty="false"
                  operator="or">
                <or>
                    <equals
                          value="org.eclipse.php.core.phpsource">
                    </equals>
                    <equals
                          value="com.aptana.editor.php.editorContext">
                    </equals>
                </or>
            </iterate>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.piece_framework.makegood.ui.views.makeGoodCategory"
            name="%views.makeGoodCategory.name">
      </category>
      <view
            category="com.piece_framework.makegood.ui.views.makeGoodCategory"
            class="com.piece_framework.makegood.ui.views.ResultView"
            icon="icons/makegood.gif"
            id="com.piece_framework.makegood.ui.views.resultView"
            name="%views.resultView.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <separator
               name="Run tests separator"
               visible="true">
         </separator>
         <command
               commandId="com.piece_framework.makegood.ui.commands.runTestFromExplorerCommand"
               icon="icons/makegood.gif"
               label="%commands.runTestFromExplorerCommand.label"
               style="push">
            <visibleWhen
                  checkEnabled="true">
               <with
                     variable="activeMenuSelection">
                  <and>
                     <or>
                        <iterate
                              ifEmpty="false"
                              operator="and">
                           <or>
                              <adapt
                                    type="org.eclipse.core.resources.IFolder">
                              </adapt>
                              <adapt
                                    type="org.eclipse.core.resources.IFile">
                              </adapt>
                              <adapt
                                    type="org.eclipse.dltk.core.IProjectFragment">
                              </adapt>
                              <adapt
                                    type="org.eclipse.dltk.core.IScriptFolder">
                              </adapt>
                              <adapt
                                    type="org.eclipse.dltk.core.IModule">
                              </adapt>
                              <adapt
                                    type="org.eclipse.dltk.core.IType">
                              </adapt>
                           </or>
                        </iterate>
                        <iterate
                              ifEmpty="false"
                              operator="and">
                           <adapt
                                 type="org.eclipse.dltk.core.IMethod">
                           </adapt>
                        </iterate></or>
                     <count
                           value="+">
                     </count>
                  </and>
               </with>
            </visibleWhen>
         </command>
         <menu
               icon="icons/makegood.gif"
               label="%menu.runTestsFor.label">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="com.piece_framework.makegood.ui.definitions.phpContentType">
               </reference>
            </visibleWhen>
            <command
                  commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInContextCommand"
                  label="%commands.runTestFromEditorInContextCommand.label"
                  style="push">
            </command>
            <command
                  commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInClassCommand"
                  label="%commands.runTestFromEditorInClassCommand.label"
                  style="push">
            </command>
            <command
                  commandId="com.piece_framework.makegood.ui.commands.runTestFromEditorInFileCommand"
                  label="%commands.runTestFromEditorInFileCommand.label"
                  style="push">
            </command>
         </menu>
         <command
               commandId="com.piece_framework.makegood.ui.commands.runRelatedTestFromEditorCommand"
               icon="icons/makegood.gif"
               label="%commands.runRelatedTestFromEditorCommand.label"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="com.piece_framework.makegood.ui.definitions.phpContentType">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="com.piece_framework.makegood.ui.commands.runAllTestsCommand"
               icon="icons/makegood.gif"
               label="%commands.runAllTestsCommand.label"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.trim.status">
         <toolbar
               id="com.piece_framework.makegood.ui.widgets.statusToolbar">
            <control
                  class="com.piece_framework.makegood.ui.widgets.ResultSquare"
                  id="com.piece_framework.makegood.ui.widgets.resultSquare">
            </control>
         </toolbar>
      </menuContribution>
   </extension>
